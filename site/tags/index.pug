extends ../../kennel/preamble.pug

block head
block main
  nav
    ol
      li
        a(href="/") Home
      li
        span Explore tags
  section
    h2 Explore tags:
    nav(class="tag__nav")
      ul(class="tag__list")
        each key in Object.keys(collections).filter((key) => !['all', 'plants'].includes(key)).sort((a, b) => a.localeCompare(b))
          li(class="tag__list_item")
            a(class="tag__anchor" href="/tags/" + key.toLowerCase()) ##{key} (#{collections[key].length})
block footer
